            </annotation>
         </attribute>
         <attribute name="fragment" type="string">
            <annotation>
               <documentation>
                  The fragment of the URI.
               </documentation>
               <appInfo>
                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
         </attribute>
      </complexType>
   </element>

   <annotation>
      <appInfo>

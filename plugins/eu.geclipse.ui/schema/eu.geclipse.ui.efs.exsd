               <appInfo>
                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
         </attribute>
         <attribute name="port" type="string">
            <annotation>
               <documentation>
                  The port of the URI.
               </documentation>
               <appInfo>
                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
         </attribute>
         <attribute name="path" type="string">
            <annotation>
               <documentation>
                  The path of the URI.
               </documentation>
               <appInfo>
                  <meta.attribute translatable="true"/>
               </appInfo>
            </annotation>
         </attribute>
         <attribute name="query" type="string">
            <annotation>
               <documentation>
                  The query of the URI.
               </documentation>
               <appInfo>
                  <meta.attribute translatable="true"/>

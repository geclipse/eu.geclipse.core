<plugin>

  <extension point="org.eclipse.emf.ecore.generated_package">
    <package 
       uri = "http:///eu/geclipse/workflow.ecore" 
       class = "eu.geclipse.workflow.IWorkflowPackage"
       genModel = "model/workflow.genmodel" /> 
  </extension>
  <extension
         id="eu.geclipse.workflow.workflowCreator"
         name="Workflow Creator"
         point="eu.geclipse.core.gridElementCreator">
      <creator
            class="eu.geclipse.workflow.internal.model.GridWorkflowCreator"
            name="Grid Workflow">
         <target
               class="eu.geclipse.core.model.IGridWorkflow"
               name="Workflow Model">
         </target>
      </creator>
   </extension>

</plugin>
